		"@uidotdev/usehooks": "2.4.1",
		"classnames": "2.5.1",
		"i18next": "24.2.1",
		"i18next-http-backend": "3.0.1",
		"lefthook": "1.10.10",
		"openapi-fetch": "0.13.7",
		"openapi-react-query": "0.4.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-i18next": "15.4.0",
		"react-router": "7.1.3",
		"reset-css": "5.0.2",
		"zod": "3.24.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "1.2.12",
