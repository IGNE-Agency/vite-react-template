		"openapi-typescript": "7.7.1",
		"prettier": "3.5.3",
		"sass": "1.84.0",
		"typescript": "5.8.3",
		"vite": "6.0.5",
		"vite-css-modules": "1.8.4",
		"vite-plugin-svgr": "4.3.0",
		"vite-tsconfig-paths": "5.1.4"
	}
