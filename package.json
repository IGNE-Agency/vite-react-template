{
	"name": "vite-react-template",
	"version": "2.2.0",
	"type": "module",
	"scripts": {
		"openapi": "bunx --bun openapi-typescript ./openapi.json -o ./src/lib/schema.gen.d.ts && bun run generate-schemas.ts ./openapi.json ./src/lib/validators.gen.ts",
		"dev": "vite",
		"build": "tsc -b && vite build",
