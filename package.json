{
	"name": "vite-react-template",
	"version": "2.2.0",
	"type": "module",
	"scripts": {
		"openapi": "bunx --bun openapi-typescript ./openapi.json -o ./src/lib/schema.gen.d.ts && bun run generate-schemas.ts ./openapi.json ./src/lib/validators.gen.ts",
		"dev": "vite",
		"check-ts": "tsc",
		"build": "tsc && vite build",
		"lint": "biome check src && prettier --check 'src/**/*.{s,}css'",
		"preview": "vite preview"
	},
	"dependencies": {
		"@tanstack/react-query": "5.75.4",
		"@uidotdev/usehooks": "2.4.1",
		"classnames": "2.5.1",
		"i18next": "25.1.1",
		"i18next-http-backend": "3.0.2",
		"openapi-fetch": "0.13.7",
		"openapi-react-query": "0.4.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-i18next": "15.5.1",
		"react-router": "7.5.3",
		"reset-css": "5.0.2",
		"zod": "3.24.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "1.2.12",
		"@types/react": "19.1.3",
		"@types/react-dom": "19.1.3",
		"@vitejs/plugin-react": "4.4.1",
		"json-refs": "3.0.15",
		"json-schema-to-zod": "2.6.1",
		"lefthook": "1.11.12",
		"openapi-types": "12.1.3",
		"openapi-typescript": "7.7.1",
		"prettier": "3.5.3",
		"sass": "1.87.0",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-css-modules": "1.8.6",
		"vite-plugin-svgr": "4.3.0",
		"vite-tsconfig-paths": "5.1.4"
	}
}
